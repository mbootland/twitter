<h1>Create a New Tweet</h1>

<%= form_with(model: @tweet, local: true) do |form| %>
  <div>
    <%= form.label :content, "Your Tweet" %><br>
    <%= form.text_area :content, placeholder: "What's happening?" %>
  </div>

  <div>
    <%= form.submit "Tweet" %>
  </div>
<% end %>

<%= link_to "Back to Tweets", root_path %>